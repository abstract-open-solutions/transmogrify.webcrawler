<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="object" meta_type="CMF Action Category">
    <object name="folderfunnelweb" meta_type="CMF Action" i18n:domain="plone">
      <property name="title" i18n:translate="">import</property>
      <property name="description" i18n:translate="">Import a website into the current folder</property>
      <property name="url_expr">string:${object/@@import}</property>
      <property
          name="available_expr">object/display_upload</property>
      <property name="permissions">
        <element value="Add portal content"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>